<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>DemoServlet</servlet-name>
        <servlet-class>DemoServlet</servlet-class>
    </servlet>
    <context-param>
        <param-name>dburl</param-name>
        <param-value>jdbc:mysql://localhost:3306/mca5</param-value>
    </context-param>
    <context-param>
        <param-name>dbuname</param-name>
        <param-value>root</param-value>
    </context-param>
    <context-param>
        <param-name>password</param-name>
        <param-value>dcs123</param-value>
    </context-param>
    <context-param>
        <param-name>dbdriver</param-name>
        <param-value>com.mysql.jdbc.Driver</param-value>
    </context-param>
    <servlet>
        <servlet-name>retrievedata</servlet-name>
        <servlet-class>retrievedata.retrievedata</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Retrievedata</servlet-name>
        <servlet-class>retrievedata.Retrievedata</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RetrieveServlets</servlet-name>
        <servlet-class>retrievedata.RetrieveServlets</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DemoServlet</servlet-name>
        <url-pattern>/DemoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>retrievedata</servlet-name>
        <url-pattern>/retrievedata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Retrievedata</servlet-name>
        <url-pattern>/Retrievedata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RetrieveServlets</servlet-name>
        <url-pattern>/RetrieveServlets</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
